{
  "name": "platform-web",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --open",
    "start-local": "ng serve --source-map --aot --open",
    "build": "ng build --configuration production --delete-output-path && (npm run sentry:sourcemaps || true)",
    "build-prod": "ng build --configuration production --delete-output-path --base-href #{deployUrl}# && (npm run sentry:sourcemaps || true)",
    "build-eur": "ng build --configuration production --delete-output-path && (npm run sentry:sourcemaps || true)",
    "analyzer": "ng build --configuration analyzer --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compile-prod": "ng build --configuration production --delete-output-path --base-href /PlatformWeb/",
    "lint": "ng lint --fix",
    "sentry:sourcemaps": "sentry-cli sourcemaps upload --release=#{version}# ./dist/platform-web"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.3",
    "@angular/cdk": "^17.1.2",
    "@angular/common": "^17.1.3",
    "@angular/compiler": "^17.1.3",
    "@angular/core": "^17.1.3",
    "@angular/forms": "^17.1.3",
    "@angular/material": "^17.1.2",
    "@angular/material-moment-adapter": "^17.1.2",
    "@angular/platform-browser": "^17.1.3",
    "@angular/platform-browser-dynamic": "^17.1.3",
    "@angular/router": "^17.1.3",
    "@angular/service-worker": "^17.1.3",
    "@babel/parser": "7.22.5",
    "@babel/traverse": "7.22.5",
    "@fingerprintjs/fingerprintjs": "^3.4.1",
    "@fingerprintjs/fingerprintjs-pro-angular": "^2.0.0",
    "@livechat/widget-angular": "^1.3.2",
    "@microsoft/signalr": "^7.0.5",
    "@ngneat/until-destroy": "^8.1.4",
    "@sentry/angular-ivy": "~7.102.0",
    "apexcharts": "3.46.0",
    "crypto-js": "^4.1.1",
    "d3": "^7.8.5",
    "google-libphonenumber": "^3.2.25",
    "hammerjs": "^2.0.8",
    "html2canvas": "^1.4.1",
    "lz-string": "^1.4.4",
    "moment": "^2.29.1",
    "ng-apexcharts": "1.9.0",
