{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "platform-web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/platform-web",
              "browser":"",
              "server":"server",
              "media": "assets"
            },
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["src/polyfills.ts"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "allowedCommonJsDependencies": [
              "angularx-qrcode",
              "qrcode",
              "crypto-js",
              "moment",
              "apexcharts",
              "paymentiq-cashier-bootstrapper",
              "html2canvas",
              "google-libphonenumber",
              "howler",
              "lz-string",
              "hammerjs"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/syspage",
              "src/thirdauth",
              "src/validator",
              "src/thirdpartygame",
              "src/chrome-not-support",
              "src/manifest.webmanifest",
              "src/release.json",
              {
                "glob": "**/*",
                "input": "node_modules/ngx-scanner-qrcode/wasm/",
                "output": "./assets/wasm/"
              }
            ],
            "styles": [
              "src/styles.scss",
              "src/assets/orignal/orignal.scss"
            ],
            "scripts": [
              "src/assets/scripts/boc.js",
              "src/assets/scripts/md5.min.js",
              "src/assets/scripts/jsencrypt.min.js"
            ],
            "serviceWorker": "ngsw-config.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "50kb",
                  "maximumError": "150kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                }
              },
              "sourceMap": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "outputHashing": "media"
            },
            "analyzer": {
              "optimization": true,
              "aot": true,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "platform-web:build:production"
            },
            "development": {
              "buildTarget": "platform-web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "platform-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/syspage",
              "src/thirdauth",
              "src/validator",
              "src/manifest.webmanifest",
              "src/release.json"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  }
}
