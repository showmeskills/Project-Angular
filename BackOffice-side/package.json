{
  "name": "platform-web-admin",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "build": "ng build --configuration production --delete-output-path --vendor-chunk && (npm run sentry:sourcemaps || npm root)",
    "build:prod": "ng build --configuration production --delete-output-path --vendor-chunk",
    "win-copy-web-config": "copy src/web-prod.config dist/PlatformWebAdmin/web.config",
    "compile-prod": "ng build --configuration production --delete-output-path --vendor-chunk --aot --base-href /PlatformWebAdmin/",
    "lint": "ng lint --fix",
    "prettier": "prettier --write **/*.{ts,js,css,html}",
    "prepare": "husky install",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject ./dist && sentry-cli sourcemaps upload ./dist || npm root"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.3",
    "@angular/cdk": "^17.3.3",
    "@angular/common": "^17.3.3",
    "@angular/compiler": "^17.3.3",
    "@angular/core": "^17.3.3",
    "@angular/forms": "^17.3.3",
    "@angular/localize": "^17.3.3",
    "@angular/material": "^17.3.3",
    "@angular/material-moment-adapter": "^17.3.3",
    "@angular/platform-browser": "^17.3.3",
    "@angular/platform-browser-dynamic": "^17.3.3",
    "@angular/router": "^17.3.3",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@ng-util/lazy": "^15.0.0",
    "@ng-util/util": "^15.0.0",
    "@popperjs/core": "^2.11.8",
    "@sentry/angular-ivy": "~7.109.0",
    "angular-svg-icon": "^17.0.0",
    "apexcharts": "^3.35.3",
    "base64-arraybuffer": "^1.0.2",
    "bignumber.js": "^9.0.2",
    "bootstrap": "^5.2.3",
    "crypto-js": "^4.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "ng-apexcharts": "~1.8.0",
    "object-path": "^0.11.5",
    "perfect-scrollbar": "^1.5.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.3",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "~17.3.3",
    "@angular/compiler-cli": "^17.3.3",
    "@babel/parser": "7.22.5",
    "@sentry/cli": "^2.23.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.12.5",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jasmine-core": "^5.1.2",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.2.5",
    "prettier-eslint": "^15.0.1",
    "typescript": "~5.4.4"
  }
}
